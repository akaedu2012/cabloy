{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ["./packages/", "./src/"],
    "exclude": ["./packages/egg-born-front/src/vendors/requirejs/require.js", "./packages/egg-born-localeutil"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(node_modules/|docs/|dist/|backend/static/|plugin/assets/js/lib/|test-party/|test-flow/|test-note/|\\.min\\.js)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "template": "./node_modules/@zhennann/docdash",
    "encoding": "utf8",
    "destination": "./docs-api/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  },
  "docdash": {
    "search": true,
    "menu": {
      "Documentations": {
        "href": "https://cabloy.com",
        "target": "_blank",
        "class": "menu-item",
        "id": "documentations_link"
      },
      "Community": {
        "href": "https://community.cabloy.com",
        "target": "_blank",
        "class": "menu-item",
        "id": "community_link"
      }
    }
  }
}
