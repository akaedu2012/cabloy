{
  "tags": {
    "allowUnknownTags": false
  },
  "source": {
    "include": ["packages", "src"],
    "includePattern": ".+\\.js(doc)?$",
    "excludePattern": "(node_modules/|docs)"
  },
  "plugins": ["plugins/markdown"],
  "opts": {
    "template": "node_modules/docdash",
    "encoding": "utf8",
    "destination": "docs-api/",
    "recurse": true,
    "verbose": true
  },
  "templates": {
    "cleverLinks": false,
    "monospaceLinks": false
  }
}
